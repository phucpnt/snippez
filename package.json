    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.17.1",
    "clarity-angular": "^0.9.2",
    "clarity-icons": "^0.9.2",
    "clarity-ui": "^0.9.2",
    "codemirror": "^5.25.2",
    "core-js": "^2.4.1",
    "cors": "^2.8.3",
    "express": "^4.15.2",
    "handlebars": "^4.0.7",
    "imports-loader": "^0.7.1",
    "mkdirp": "^0.5.1",
    "mutationobserver-shim": "^0.3.2",
